{
  "compilerOptions": {
    "target": "ES2016",
    "module": "CommonJS",
    "moduleResolution": "node",
    "outDir": "./dist",
    "rootDir": "./src",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": false,
    "sourceMap": false,
    //"noImplicitAny": false,
    "skipLibCheck": true,
    "noUnusedParameters": false,
    "resolveJsonModule": true,
    "allowJs": true
  }
}